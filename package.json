{
  "name": "gatsby-starter",
  "description": "Gatsby starter with semantic ui and pwa plugins.",
  "version": "1.0.0",
  "engine": "node >= 6.10.0",
  "dependencies": {
    "gatsby": "^1.9.45",
    "gatsby-link": "^1.6.17",
    "gatsby-plugin-manifest": "^1.0.8",
    "gatsby-plugin-offline": "^1.0.9",
    "gatsby-plugin-react-helmet": "^1.0.5",
    "gatsby-plugin-sitemap": "^1.2.5",
    "gatsby-plugin-typography": "^1.7.10",
    "react-apollo": "^1.4.16",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.74.2",
    "typography-theme-github": "^0.15.10"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "deploy": "npm run build ; npm run s3sync ; npm run uncache",
    "s3sync": "aws s3 rm --recursive s3://dakotahipp.com && aws s3 cp --recursive public/ s3://dakotahipp.com",
    "uncache": "aws cloudfront create-invalidation --distribution-id E1945YLB5XCVFY --paths /index.html /*.js",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.6.1"
  }
}
